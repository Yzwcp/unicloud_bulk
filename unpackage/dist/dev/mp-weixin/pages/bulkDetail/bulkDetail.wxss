.bulk {
  background: #f6f6f6;
}
.bulk .bulk_body > view {
  border-radius: 20rpx;
  background: white;
}
.bulk .body_detail .detail_content {
  display: flex;
  flex-direction: column;
}
.bulk .body_detail .detail_content .content_text {
  font-size: 22rpx;
  padding-top: 20rpx;
  align-self: flex-start;
  padding: 20rpx 40rpx 20rpx 40rpx;
}
.bulk .body_detail .detail_content image {
  height: 700rpx;
  width: 100%;
}
.bulk .body_detail .detail_foot {
  height: 128rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
  color: white;
  position: relative;
}
.bulk .body_detail .detail_foot .foot_price {
  position: absolute;
  left: 30rpx;
  bottom: 20rpx;
}
.bulk .body_detail .detail_foot .foot_price text:nth-child(2) {
  display: inline-block;
  font-size: 56rpx;
}
.bulk .body_detail .detail_foot .foot_num {
  position: absolute;
  left: 140rpx;
  bottom: 26rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 80rpx;
}
.bulk .body_detail .detail_foot .foot_num text:nth-child(1) {
  display: inline-block;
  font-size: 18rpx;
  padding: 2rpx 8rpx;
  background: #b8213f;
  border-radius: 30rpx;
  text-align: center;
  line-height: 30prx;
}
.bulk .body_detail .detail_foot .foot_num text:nth-child(2) {
  display: inline-block;
  font-size: 28rpx;
  text-decoration: line-through;
}
.bulk .body_detail .detail_foot .foot_time {
  position: absolute;
  right: 30rpx;
  bottom: 26rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 80rpx;
}
.bulk .body_countdown {
  padding: 30rpx 30rpx 30rpx 30rpx;
  margin: 30rpx 30rpx 0 30rpx;
  font-size: 26rpx;
}
.bulk .body_countdown .people_title {
  margin-bottom: 30rpx;
  font-weight: 700;
  font-size: 30rpx;
}
.bulk .body_countdown .goodstype {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
  justify-content: space-between;
}
.bulk .body_countdown .goodstype .title {
  color: #999;
}
.bulk .body_items {
  margin: 30rpx 30rpx 0 30rpx;
}
.bulk .body_items ._div ._span {
  padding: 40rpx;
  display: inline-block;
}
.bulk .bottom_bar {
  height: 50rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  width: 100vw;
  box-sizing: border-box;
  height: 100rpx;
  align-items: center;
  background: white;
  box-shadow: 0px 9rpx 10rpx 6rpx black;
  justify-content: space-between;
  padding: 0 30rpx;
}
.bulk .bottom_bar .img {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
  color: grey;
}
.bulk .bottom_bar .img image {
  width: 40rpx;
  height: 40rpx;
}
.bulk .bottom_bar .create {
  display: inline-block;
  width: 200rpx;
  border-radius: 100rpx;
  line-height: 80rpx;
  background-color: #f9464a;
  color: white;
  text-align: center;
}
.indicator {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.indicator__dot {
  height: 6px;
  width: 6px;
  border-radius: 100px;
  background-color: rgba(255, 255, 255, 0.35);
  margin: 0 5px;
  transition: background-color 0.3s;
}
.indicator__dot--active {
  background-color: #ffffff;
}
.indicator-num {
  padding: 2px 0;
  background-color: rgba(0, 0, 0, 0.35);
  border-radius: 100px;
  width: 35px;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.indicator-num__text {
  color: #FFFFFF;
  font-size: 12px;
}

